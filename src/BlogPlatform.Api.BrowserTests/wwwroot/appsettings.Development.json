{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "API": {
    "Urls": {
      "BaseAddress": "https://localhost:7039/",
      "Identity": {
        "UserInfo": "api/identity",
        "BasicLogin": "api/identity/login/basic",
        "BasicSignUp": "api/identity/signup/basic",
        "SendVerifyEmail": "api/identity/email/verify",
        "OAuthLogin": "api/identity/login/oauth",
        "OAuthSignUp": "api/identity/signup/oauth",
        "OAuthAdd": "api/identity/add/oauth",
        "OAuthRemove": "api/identity/oauth/{0}",
        "Logout": "api/identity/logout",
        "Refresh": "api/identity/refresh",
        "ChangePassword": "api/identity/password/change",
        "ResetPassword": "api/identity/password/reset",
        "ChangeName": "api/identity/name",
        "ChangeEmail": "api/identity/email/change",
        "FindId": "api/identity/id/find",
        "Withdraw": "api/identity/withdraw",
        "CancelWithdraw": "api/identity/withdraw/cancel"
